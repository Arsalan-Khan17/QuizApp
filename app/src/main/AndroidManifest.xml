<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.riaz.quizapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TeacherLogin" />
        <activity android:name=".AddQuizViewResult" />
        <activity android:name=".CreateQuiz" />
        <activity android:name=".AddQuestions" />
        <activity android:name=".StudentLogin" />
        <activity android:name=".Quiz" />
        <activity android:name=".Result" />
        <activity android:name=".CreateNewAccount"></activity>
    </application>

</manifest>